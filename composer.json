{
  "name": "kuzmich/cache",
  "description": "Обёртка для doctrine/cache совместимая с PSR6",
  "keywords": ["psr", "cache", "caching", "key", "value", "store", "psr6", "psr-6", "psr-cache", "doctrine"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey V. Kuzin",
      "email": "sergey@kuzin.name",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "doctrine/cache": "^1.3"
  },
  "provide": {
    "psr/cache-implementation": "1.0.0"
  },
  "suggest": {
    "doctrine/cache": ""
  },
  "replace": {
    "psr/cache": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Psr\\Cache\\": "src/Psr/Cache",
      "Cache\\": "src"
    }
  },
  "extra": {
     "branch-alias": {
       "dev-master": "0.1.x-dev"
     }
   }
}
